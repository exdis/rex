partial
xkb_compatibility "super_mappings" {
    interpret KP_Left {
        repeat= True;
        action= RedirectKey(keycode=<LEFT>, clearmods=Super);
    };
    interpret KP_Right {
        repeat= True;
        action= RedirectKey(keycode=<RGHT>, clearmods=Super);
    };
    interpret KP_Up {
        repeat= True;
        action= RedirectKey(keycode=<UP>, clearmods=Super);
    };
    interpret KP_Down {
        repeat= True;
        action= RedirectKey(keycode=<DOWN>, clearmods=Super);
    };
    interpret KP_Prior {
        repeat= True;
        action= RedirectKey(keycode=<PGUP>, clearmods=Super);
    };
    interpret KP_Next {
        repeat= True;
        action= RedirectKey(keycode=<PGDN>, clearmods=Super);
    };
    interpret KP_Home {
        repeat= True;
        action= RedirectKey(keycode=<HOME>, clearmods=Super);
    };
    interpret KP_End {
        repeat= True;
        action= RedirectKey(keycode=<END>, clearmods=Super);
    };
    interpret KP_Enter {
        repeat= True;
        action= RedirectKey(keycode=<RTRN>, clearmods=Super);
    };
    interpret KP_Subtract {
        repeat= True;
        action= RedirectKey(keycode=<BKSP>, clearmods=Super);
    };
    interpret KP_Delete {
        repeat= True;
        action= RedirectKey(keycode=<DELE>, clearmods=Super);
    };
};
